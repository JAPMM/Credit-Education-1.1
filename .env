# Database URL (Postgres)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# NextAuth (JWT Secret)
NEXTAUTH_SECRET="YOUR_RANDOM_SECRET"

# NextAuth Google Provider
GOOGLE_CLIENT_ID="your-google-client-id"
